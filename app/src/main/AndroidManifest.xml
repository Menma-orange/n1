            android:label="Profile" />
        <activity
            android:name=".news"
            android:label="News" />
        <activity
            android:name=".menu"
            android:label="Menu" />
        <activity android:name=".registr" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>